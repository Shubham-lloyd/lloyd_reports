  else ((B.Rate__Stduom * B.Quantity__StdUom) - (nvl((((NVL(B.QUANTITY, 0) * NVL(B.RATE__STDUOM, 0) * NVL(A.EXCH_RATE, 0)) * nvl(B.DISCOUNT, 0)) / 100), 0) + nvl(FN_RGET_TAX_DET('S-RET', B.Tran_Id, cast(B.line_no AS CHAR(3)), 'DISC_GST', 'T') * (- 1), 0)) )
